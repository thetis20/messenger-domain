{
  "name": "thetis20/messenger-domain",
  "type": "library",
  "license": "MIT",
  "description": "domain of messenger application",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "beberlei/assert": "^3.3",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^1.33",
    "symfony/dotenv": "^7.1",
    "symfony/uid": "^7.1"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Messenger\\Domain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Messenger\\Domain\\Tests\\": "tests/",
      "Messenger\\Domain\\TestsIntegration\\": "tests-integration/"
    }
  },
  "authors": [
    {
      "name": "Arnaud Picard",
      "email": "arnaud.picard@lilo.org"
    }
  ],
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src tests tests-integration --level 6",
    "test": "phpunit",
    "tests": "phpunit"
  }
}
